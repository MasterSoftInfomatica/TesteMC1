      <setting name="UsuarioServidorSmtp" serializeAs="String">
        <value>[usuario_smtp]</value>
      </setting>
      <setting name="SenhaServidorSmtp" serializeAs="String">
        <value>[senha_smtp]</value>
      </setting>
      <setting name="EnderecoEmailDe" serializeAs="String">
        <value>[email_de]</value>
